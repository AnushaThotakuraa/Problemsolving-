import React from "react";
const FruitList=()=>{
  const fruits=['apple','cherry','orangr'];
    return(
        <div>
            <h1>Fruits list</h1>
            <ul>
                {fruits.map((fruit,index)=>(
                    <li key={index}>{fruit}</li>
                ))}
            </ul>
        </div>
    )
  
}
export default FruitList;
